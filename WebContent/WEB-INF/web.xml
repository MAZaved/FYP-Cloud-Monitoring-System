		<display-name>GetGraph</display-name>
		<servlet-name>GetGraph</servlet-name>
		<servlet-class>controller.GetGraph</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetGraph</servlet-name>
		<url-pattern>/GetGraph</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
