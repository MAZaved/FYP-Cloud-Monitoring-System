  <servlet>
    <display-name>StartStopServlet</display-name>
    <servlet-name>StartStopServlet</servlet-name>
    <servlet-class>controller.StartStopServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>HighChartServlet</display-name>
